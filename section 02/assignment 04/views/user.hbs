<main>
    {{#if hasUsers }}
        <h1>Users</h1>
        <p>List of users</p>
            {{#each users}}
            <div class="list">
                <ul class="list-ul">
                    <li class="list-ul-li">{{ this.title }}</li>
                </ul>
            </div>
            {{/each}}
    {{/if}}
    </main>